#!/bin/bash
echo "algoritmo utilizado,numero a buscar,tamano de n,tiempo real,encontrado" > registro_tiempo.csv
 
A=(322486 14700764 3128036 6337399 61396
10393545 2147445644 1295390003 450057883 187645041
1980098116 152503 5000 1493283650 214826 1843349527
1360839354 2109248666 2147470852 0)

n=(100 1000 5000 10000 50000 100000 200000 400000 600000
800000 1000000 2000000 3000000 4000000 5000000 6000000
7000000 8000000 9000000 10000000)

algoritmos=(1 2 3 4 5)
lim_init=(2)
lim_end=(2)
for x in ${algoritmos[*]}
do
	for y in ${n[*]}
	do
		for z in ${A[*]}
		do
		if [ $x -eq 3 ]
		
		then
		#llamada a programa pasando como argumentos x,y,z
			./prueba  $y $z $x < numeros10millones.txt >> registro_tiempo.csv
		else
			./prueba  $y $z $x< 10millones.txt >> registro_tiempo.csv
		fi
		let lim_end=lim_end+1
		done
		let lim_end=lim_end-1
		echo " , ,promedio,=sum(D$lim_init:D$lim_end)/count(D$lim_init:D$lim_end), " >> registro_tiempo.csv		
		let lim_end=lim_end+2
		let lim_init=lim_end
	done
done

